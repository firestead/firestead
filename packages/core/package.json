{
    "name": "firestead",
    "version": "0.0.1",
    "description": "Firestead Core Module",
    "repository": "disedia/firestead",
    "license": "MIT",
    "type": "module",
    "exports": {
      ".": {
        "types": "./dist/types.d.ts",
        "import": "./dist/module.mjs",
        "require": "./dist/module.cjs"
      }
    },
    "publishConfig": {
      "access": "public"
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
      "dist"
    ],
    "scripts": {
      "prepack": "nuxt-module-build build",
      "prepack:stub": "nuxt-module-build build --stub"
    },
    "dependencies": {
        "firebase": "^10.11.1",
        "vuefire": "^3.1.23",
        "nuxt-vuefire": "^1.0.2",
        "firebase-admin": "12.1.0",
        "@nuxt/ui": "^2.15.2",
        "@iconify-json/flagpack": "1.1.14",
        "firebase-functions": "^5.0.1",
        "@firebase/app-types": "^0.9.1",
        "@firebase/app": "^0.10.2",
        "@nuxt/kit": "^3.11.2",
        "@nuxtjs/i18n": "^8.3.1",
        "@firestead/auth": "*",
        "@firestead/i18n": "*"
    },
    "devDependencies": {

    }
}